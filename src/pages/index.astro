---
import Layout from "~/layouts/PageLayout.astro";

import FAQs from "~/components/widgets/FAQs.astro";

import DocsToMarkdownHero from "../components/landing/DocsToMarkdownHero.astro";
import GCPPartnerSection from "~/components/common/GCPPartnerSection.astro";

const metadata = {
  title: "Google Docs to Markdown",
  ignoreTitleTemplate: true,
  description:
    "Convert Google Docs to Markdown, Convert Google Docs to HTML, Convert Markdown to Google Docs, Convert HTML to Google Docs",
};
---

<link
  rel="stylesheet"
  href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.0/css/all.min.css"
/>

<Layout metadata={metadata}>
  <!-- Hero Widget ******************* -->
